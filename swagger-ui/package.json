{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "codegen": "for f in ../specs/*/openapi.yaml; do name=$(basename $(dirname \"$f\")); redocly bundle \"$f\" -o \"public/specs/$name.yaml\"; done"
  },
  "devDependencies": {
    "@types/swagger-ui": "5.21.1",
    "typescript": "5.9.3",
    "vite": "7.2.2"
  },
  "dependencies": {
    "swagger-ui": "5.30.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@scarf/scarf"
    ],
    "onlyBuiltDependencies": [
      "@tree-sitter-grammars/tree-sitter-yaml",
      "core-js-pure",
      "esbuild",
      "tree-sitter",
      "tree-sitter-json"
    ]
  }
}
